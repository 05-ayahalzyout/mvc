
@{
   ViewBag.Title = "Index";
}

<h2>Index</h2>

<table class="table table-striped table-hover"  >
    <thead><tr><th>ID</th><th>Name</th><th>Major</th><th>Faculity</th></tr></thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr><td>@item.ID</td><td>@item.Name</td><td>@item.Major</td><td>@item.Faculity</td></tr>
        }
    </tbody>
</table>